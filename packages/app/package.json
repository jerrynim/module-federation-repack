{
	"name": "repack",
	"packageManager": "yarn@3.1.1",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"start:host": "react-native webpack-start --port 8081",
		"start:shell": "yarn workspace app_shell run start",
		"start:component": "yarn workspace app_component run start",
		"start:chart": "yarn workspace app_chart run start",
		"pod_install:host": "cd ios && pod install",
		"pod_install:component": "yarn workspace app_component run pod install",
		"pod_install:chart": "yarn workspace app_chart run pod install",
		"dev": "concurrently -P 'yarn:start:*'",
		"pod_install": "concurrently -P 'yarn:pod_install:*'",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"analyze:ios": "ANALYZE=1 react-native webpack-bundle --platform ios --entry-file index.js --dev fals",
		"analyze:android": "ANALYZE=1 react-native webpack-bundle --platform android --entry-file index.js --dev fals",
		"bundle:ios": "react-native webpack-bundle --platform ios --entry-file index.js  --bundle-output ./ios/main.jsbundle  --reset-cache --dev=false",
		"bundle:android": "react-native webpack-bundle --platform android --entry-file index.js --dev=false",
		"webpack": "webpack -c webpack.config.mjs --env platform=ios --env mode=production",
		"bundle_build_android": "mkdir android/app/src/main/assets/ && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
	},
	"devDependencies": {
		"@babel/core": "^7.22.9",
		"@babel/runtime": "^7.22.6",
		"@callstack/repack": "^3.3.1",
		"@react-native-community/eslint-config": "^2.0.0",
		"@react-native-masked-view/masked-view": "^0.2.9",
		"@react-navigation/drawer": "^6.6.3",
		"@react-navigation/native": "^6.1.7",
		"@react-navigation/native-stack": "^6.9.13",
		"@react-navigation/stack": "*",
		"@rivascva/react-native-code-editor": "^1.2.2",
		"@statoscope/webpack-plugin": "^5.27.0",
		"@svgr/webpack": "^6.5.1",
		"@tsconfig/react-native": "^2.0.2",
		"@types/react": "^18.0.26",
		"@types/react-native": "^0.69.6",
		"babel-loader": "^9.1.3",
		"babel-plugin-module-resolver": "^4.1.0",
		"browserify-zlib": "^0.2.0",
		"concurrently": "^8.2.0",
		"metro-react-native-babel-preset": "0.72.3",
		"react": "18.2.0",
		"react-jsx-parser": "^1.29.0",
		"react-native": "0.71.10",
		"react-native-device-info": "^10.8.0",
		"react-native-dotenv": "^3.4.9",
		"react-native-gesture-handler": "^2.12.1",
		"react-native-haptic-feedback": "^1.14.0",
		"react-native-linear-gradient": "^2.6.2",
		"react-native-reanimated": "1.13.4",
		"react-native-safe-area-context": "^4.7.1",
		"react-native-screens": "^3.23.0",
		"react-native-svg": "^12.3.0",
		"react-native-webview": "11.23.1",
		"recoil": "^0.7.7",
		"stream-browserify": "^3.0.0",
		"stream-http": "^3.2.0",
		"terser-webpack-plugin": "^5.3.6",
		"typescript": "^4.6.3",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	}
}
