{
	"name": "app_shell",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "react-native webpack-start --port 9000",
		"pod_install": "cd ./ios && pod install",
		"bundle:ios": "react-native webpack-bundle --platform ios --entry-file ./index.js --reset-cache --dev false",
		"bundle:android": "react-native webpack-bundle --platform android --entry-file index.js --dev false",
		"analyze:ios": "ANALYZE=1 react-native webpack-bundle --platform ios --entry-file index.js --dev fals",
		"analyze:android": "ANALYZE=1 react-native webpack-bundle --platform android --entry-file index.js --dev fals"
	},
	"devDependencies": {
		"@babel/core": "^7.22.9",
		"@babel/runtime": "^7.22.6",
		"@callstack/repack": "^3.3.1",
		"@react-native-community/eslint-config": "^2.0.0",
		"@types/react": "^18.0.26",
		"@types/react-native": "^0.69.6",
		"babel-loader": "^9.1.3",
		"metro-react-native-babel-preset": "0.72.3",
		"react": "18.2.0",
		"react-native": "0.71.10",
		"react-native-device-info": "^10.8.0",
		"recoil": "^0.7.7",
		"terser-webpack-plugin": "^5.3.6",
		"typescript": "^4.6.3",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	}
}
